{
  "name": "jp-equity-backtest-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "test:unit": "vitest run --coverage",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:bench": "vitest bench --config vitest.benchmark.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:performance": "playwright test tests/e2e/performance.spec.ts",
    "size:snapshot": "rollup-plugin-size-snapshot",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:bench && npm run test:e2e",
    "test:ci": "npm run test:all && npm run size:snapshot",
    "generate:dummy-arrow": "node scripts/generate-dummy-arrow.js"
  },
  "dependencies": {
    "@duckdb/duckdb-wasm": "1.29.0",
    "@google/generative-ai": "0.5.0",
    "@tailwindcss/vite": "^4.1.7",
    "@tanstack/react-table": "^8.21.3",
    "apache-arrow": "^16.0.0",
    "idb": "8.0.0",
    "openai": "4.7.0",
    "react": "18.3.0",
    "react-dom": "18.3.0",
    "recharts": "^2.15.3",
    "shadcn-ui": "^0.9.5",
    "zod": "^3.22.2",
    "zod-to-json-schema": "3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@playwright/test": "^1.52.0",
    "@types/node": "^22.15.23",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "@vitejs/plugin-react": "^4.5.0",
    "@vitest/coverage-v8": "^1.6.0",
    "autoprefixer": "^10.4.21",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "fast-check": "^3.23.2",
    "globals": "^16.2.0",
    "happy-dom": "^14.12.0",
    "jsdom": "^24.1.3",
    "playwright": "^1.44.0",
    "postcss": "^8.5.3",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "tailwindcss": "^4.1.7",
    "typescript": "5.4.5",
    "uuid": "^11.1.0",
    "vite": "^6.3.5",
    "vitest": "^1.6.0"
